<?xml version="1.0" standalone="yes"?>
<NewDataSet>
  <hinttable>
    <option>-d</option>
    <description>Löst Adressen nicht in Hostnamen auf.</description>
  </hinttable>
  <hinttable>
    <option>-h</option>
    <description>Max. Abschnitte  Maximale Anzahl an Abschnitten bei Zielsuche</description>
  </hinttable>
